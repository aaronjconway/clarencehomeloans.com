{
	"name": "clarencehomeloans.com",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "npm run sass:build && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"sass:dev": "sass --watch --embed-source-map src/styles:dist",
		"sass:build": "sass --no-source-map --style=compressed src/styles:dist"
	},
	"devDependencies": {
		"@iconify-json/openmoji": "^1.2.8",
		"@splidejs/svelte-splide": "^0.2.9",
		"@sveltejs/adapter-cloudflare": "^6.0.1",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.20.2",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/vite": "^4.0.17",
		"@uppy/core": "^4.4.3",
		"@uppy/dashboard": "^4.3.2",
		"@uppy/svelte": "^4.3.0",
		"@uppy/tus": "^4.2.2",
		"date-fns": "^4.1.0",
		"font-awesome": "^4.7.0",
		"globals": "^16.0.0",
		"mdsvex": "^0.12.3",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-slug": "^6.0.0",
		"remark-abbr": "^1.4.2",
		"sass": "^1.86.0",
		"svelte": "^5.25.3",
		"svelte-check": "^4.1.5",
		"tailwindcss": "^4.0.17",
		"typescript": "^5.8.2",
		"unplugin-icons": "^22.1.0",
		"vite": "^6.2.3"
	},
	"type": "module",
	"dependencies": {
		"@directus/sdk": "^19.1.0",
		"@qwik.dev/partytown": "^0.11.0",
		"@supabase/ssr": "^0.6.1",
		"@supabase/supabase-js": "^2.49.4"
	}
